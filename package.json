{
  "name": "vue-syncers-feathers",
  "version": "0.2.0",
  "description": "Synchronises feathers services with vue objects, updated in real time",
  "license": "MIT",
  "main": "dist/vue-syncers-feathers.common.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run build:commonjs",
    "build:commonjs": "rollup -c",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "ci:test": "npm run lint && npm run coverage",
    "clean": "rimraf dist/*.* coverage/**/*.*",
    "coverage": "nyc npm run unit",
    "coverage-html": "rimraf coverage/**/*.* && npm run coverage && nyc report --reporter=html",
    "lint": "xo",
    "serve-example": "node example/server.js",
    "test": "npm run lint && npm run unit",
    "unit": "ava"
  },
  "keywords": [
    "vue",
    "vuejs",
    "feathers",
    "feathersjs"
  ],
  "author": "t2t2 <taavot@gmail.com>",
  "repository": "t2t2/vue-syncers-feathers",
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.6.5",
    "babel-runtime": "^5.8.34",
    "body-parser": "^1.15.0",
    "country-data": "0.0.24",
    "coveralls": "^2.11.8",
    "css-loader": "^0.23.1",
    "feathers": "^2.0.0",
    "feathers-commons": "^0.6.2",
    "feathers-memory": "^0.6.3",
    "feathers-rest": "^1.2.3",
    "feathers-socketio": "^1.3.3",
    "json-loader": "^0.5.4",
    "lodash": "^4.6.1",
    "mock-socket": "^2.0.0",
    "nyc": "^6.0.0",
    "rimraf": "^2.5.2",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.4.0",
    "socket.io-client": "^1.4.5",
    "uberproto": "^1.2.0",
    "vue": "^1.0.17",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.0",
    "vue-loader": "^8.2.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1",
    "xo": "^0.13.0"
  },
  "xo": {
    "envs": [
      "node",
      "browser"
    ],
    "ignores": [
      "example/*.build.js"
    ],
    "semicolon": false,
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ]
    }
  },
  "ava": {
    "fail-fast": true,
    "files": "test/**/*.test.js"
  }
}
