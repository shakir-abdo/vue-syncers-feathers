{
  "name": "vue-syncers-feathers",
  "version": "0.1.0",
  "description": "Synchronises feathers services with vue objects, updated in real time",
  "license": "MIT",
  "main": "dist/vue-syncers-feathers.common.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run build:commonjs",
    "build:commonjs": "rollup -c",
    "clean": "rimraf dist/*.* coverage/**/*.*",
    "coverage": "nyc npm run unit",
    "coverage-html": "rimraf coverage/**/*.* && npm run coverage && nyc report --reporter=html",
    "lint": "xo",
    "serve-example": "node example/server.js",
    "test": "npm run lint && npm run unit",
    "unit": "ava"
  },
  "keywords": [
    "vue",
    "vuejs",
    "feathers",
    "feathersjs"
  ],
  "author": "t2t2 <taavot@gmail.com>",
  "dependencies": {
    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^0.10.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-2": "^6.3.13",
    "babel-runtime": "^5.8.34",
    "body-parser": "^1.14.2",
    "country-data": "0.0.24",
    "css-loader": "^0.23.1",
    "feathers": "^1.3.0",
    "feathers-client": "^0.5.1",
    "feathers-commons": "^0.5.0",
    "feathers-memory": "^0.5.1",
    "mock-socket": "^2.0.0",
    "nyc": "^5.3.0",
    "rimraf": "^2.5.0",
    "rollup": "^0.25.1",
    "rollup-plugin-babel": "^2.3.9",
    "socket.io": "^1.4.4",
    "socket.io-client": "^1.4.4",
    "uberproto": "^1.2.0",
    "vue": "^1.0.15",
    "vue-hot-reload-api": "^1.2.2",
    "vue-html-loader": "^1.1.0",
    "vue-loader": "^8.0.2",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "xo": "^0.12.1"
  },
  "xo": {
    "envs": [
      "node",
      "browser"
    ],
    "semicolon": false,
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ]
    }
  },
  "ava": {
    "fail-fast": true,
    "files": "test/**/*.test.js"
  }
}
